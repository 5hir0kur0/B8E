<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:syntaxTheme xmlns:ns2="https://github.com/5hir0kur0/B8E/tree/master/src/gui">
<!-- B8E theme template -->
<!-- Base16 <%= @scheme %> Light, by <%= @author %> -->
<!-- template for base16-builder by Noxgrim -->
    <styleMap>
        <entry>
            <key>asm</key>
            <value>
                <style pattern="(\S+)">
                    <attributes foreground="#FF<%= @base["08"]["hex"] %>" strikeThrough="true"/>
                </style>
                <style pattern="\((.*)\),|\((.*)\)" flags="2">
                    <attributes foreground="#FF<%= @base["04"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="(\().*(\)),|(\().*(\))" flags="2">
                    <attributes foreground="#FF<%= @base["0C"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="\w(\.)\w">
                    <attributes foreground="#FF<%= @base["09"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="(?:(?:(?&lt;=\w)\.|(?&lt;=[\w,])\s*[-+#/]?)\b([\w&amp;&amp;[\D]]\w*)\b)">
                    <attributes foreground="#FF<%= @base["0C"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="(?&lt;=[\w,])\s*+([/#+-])[\w&quot;'\(]+">
                    <attributes foreground="#FF<%= @base["0B"]["hex"] %>" italic="false"/>
                </style>
                <style pattern="(?&lt;=[\w,])\s*([#+-]\b(?:a(?:cc)?|c)|(?:[/#+-]\bab|dptr|r[0-7]))\b" flags="2">
                    <attributes foreground="#FF<%= @base["08"]["hex"] %>" strikeThrough="true"/>
                </style>
                <style pattern="(?&lt;=[\w,])\s*(?:/?\b(a(?:cc)?|c)|\b(ab|dptr|r[0-7]))\b" flags="2">
                    <attributes foreground="#FF<%= @base["0A"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="(?&lt;=[\w,])\s*(@(?:a\s*\+\s*dptr|a\s*\+\s*pc|dptr|r[01]))\b" flags="2">
                    <attributes foreground="#FF<%= @base["09"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^(\s*(?:[\$#\.].*?|[\$#\.]?(?:if|elif|else|endif|regex|end|file|line|include|org|end|d[bws])\s+.*?|\S*?(?&lt;!(?:[al]?call|[als]?jmp|mov[cx]?|reti?|swap|xchd?|addc?|subb|mul|div|da|setb|clr|cpl|anl|[ox]rl|rlc?|rrc?|nop|push|pop|inc|dec|cjne|djnz|jn?[bcz]|jbc?)\b)\s+(?:equ|set|bit|code|[ix]?data)\s+.*?))\s*(?:(?&lt;!\\);|$)" flags="2">
                    <attributes background="#FF<%= @base["05"]["hex"] %>"/>
                </style>
                <style pattern="^\s*(?:([\$#\.]?\s*(?:org|end|d[bws]|if|elif|else|endif|regex|end|file|line|include|equ|set|bit|code|[ix]?data))|\S*?(?&lt;!(?:[al]?call|[als]?jmp|mov[cx]?|reti?|swap|xchd?|addc?|subb|mul|div|da|setb|clr|cpl|anl|[ox]rl|rlc?|rrc?|nop|push|pop|inc|dec|cjne|djnz|jn?[bcz]|jbc?)\b)\s+(equ|set|bit|code|[ix]?data))" flags="2">
                    <attributes foreground="#FF<%= @base["09"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^\s*[\$#\.]\s*?include\s+?&lt;(.*?)&gt;" flags="2">
                    <attributes foreground="#FF<%= @base["0E"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^\s*[\$#\.]?\s*?include\s+(&lt;).*?(&gt;)" flags="2">
                    <attributes foreground="#FF<%= @base["03"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^\s*\b([\w&amp;&amp;[\D]]\w*)\b\s*(?:equ|set|bit|code|[ix]?data)\s*" flags="2">
                    <attributes foreground="#FF<%= @base["0C"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="(?:(?&lt;=\w)\.|(?&lt;=[\w,])\s*[-+#/]?)\b(\d+\w*?)[boqdh]?\b" flags="2">
                    <attributes foreground="#FF<%= @base["08"]["hex"] %>" italic="true"/>
                </style>
                <style pattern="(?:(?&lt;=\w)\.|(?&lt;=[\w,])\s*[-+#/]?)\b(0x[\da-f]*|\d+[\da-f]*h)\b" flags="2">
                    <attributes foreground="#FF<%= @base["0E"]["hex"] %>" italic="false" strikeThrough="false"/>
                </style>
                <style pattern="(?:(?&lt;=\w)\.|(?&lt;=[\w,])\s*[-+#/]?)\b([0-7]+[oq])\b" flags="2">
                    <attributes foreground="#FF<%= @base["0E"]["hex"] %>" italic="false" strikeThrough="false"/>
                </style>
                <style pattern="(?:(?&lt;=\w)\.|(?&lt;=[\w,])\s*[-+#/]?)\b([01]+b)\b" flags="2">
                    <attributes foreground="#FF<%= @base["0E"]["hex"] %>" italic="false" strikeThrough="false"/>
                </style>
                <style pattern="(?:(?&lt;=\w)\.|(?&lt;=[\w,])\s*[-+#/]?)\b(\d+d?)\b" flags="2">
                    <attributes foreground="#FF<%= @base["0E"]["hex"] %>" italic="false" strikeThrough="false"/>
                </style>
                <style pattern="(?:(?&lt;=\w)\.|(?&lt;=[\w,])\s*[-+#/]?)\b(?:(0x)\w*?|\d+\w*?([boqdh])?)\b" flags="2">
                    <attributes foreground="#FF<%= @base["03"]["hex"] %>" italic="false" strikeThrough="false"/>
                </style>
                <style pattern="^(?:\s*(?:[\w&amp;&amp;[\D]]\w*\s*:\s*)+)?\s*(?:[al]?call|[als]?jmp|mov[cx]?|reti?|swap|xchd?|addc?|subb|mul|div|da|setb|clr|cpl|anl|[ox]rl|rlc?|rrc?|nop|push|pop|inc|dec|cjne|djnz|jn?[bcz]|jbc?)\b\s+[^\s]*?\s*(,)\s*[^\s,]+(?:(,)\s*[^\s,]+)?" flags="2">
                    <attributes foreground="#FF<%= @base["09"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^(?:\s*(?:[\w&amp;&amp;[\D]]\w*\s*:\s*)+)?\s*((?:[al]?call|[als]?jmp|mov[cx]?|reti?|swap|xchd?|addc?|subb|mul|div|da|setb|clr|cpl|anl|[ox]rl|rlc?|rrc?|nop|push|pop|inc|dec|cjne|djnz|jn?[bcz]|jbc?)\b)" flags="2">
                    <attributes foreground="#FF<%= @base["09"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^\s*((?:[\w&amp;&amp;[\D]]\w*\s*:\s*)+)">
                    <attributes foreground="#FF<%= @base["0A"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="((?&lt;!\\)&quot;.*?(?&lt;!\\)&quot;|(?&lt;!\\)'.*?(?&lt;!\\)')">
                    <attributes foreground="#FF<%= @base["0D"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="\s*(;.*)$">
                    <attributes foreground="#FF<%= @base["05"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="\s*;.*?\b(TODO\b.*)$" flags="2">
                    <attributes background="#FF<%= @base["06"]["hex"] %>" foreground="#FF<%= @base["0A"]["hex"] %>" italic="true" strikeThrough="false"/>
                </style>
            </value>
        </entry>
        <entry>
            <key>hex</key>
            <value>
                <style pattern="^(.*?)\s*$">
                    <attributes foreground="#FF<%= @base["04"]["hex"] %>"/>
                </style>
                <style pattern="^:(.*?)\s*$">
                    <attributes foreground="#FF<%= @base["08"]["hex"] %>" strikeThrough="true"/>
                </style>
                <style pattern="^(:)">
                    <attributes foreground="#FF<%= @base["04"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^:([\da-f]{2})" flags="2">
                    <attributes foreground="#FF<%= @base["09"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^:..([\da-f]{4})" flags="2">
                    <attributes foreground="#FF<%= @base["04"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^:.{6}(0[0-5])">
                    <attributes foreground="#FF<%= @base["08"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^:.{8}((?:[\da-f]{2}(?!$\s*))*)" flags="2">
                    <attributes foreground="#FF<%= @base["0E"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^:.{8}(?:..)*?([\da-f]{2})\s*$" flags="2">
                    <attributes background="#FF<%= @base["0A"]["hex"] %>" foreground="#FF<%= @base["04"]["hex"] %>" strikeThrough="false"/>
                </style>
                <style pattern="^:(00000001)ff\s*$" flags="2">
                    <attributes background="#FF<%= @base["0C"]["hex"] %>" foreground="#FF<%= @base["01"]["hex"] %>" strikeThrough="false"/>
                </style>
            </value>
        </entry>
        <entry>
            <key>properties</key>
            <value>
                <style pattern="^\s*(?:[\w\.\-]|\\\s)+\s*[=:]\s*([^\s].*)$|^\s*([^\s].*)\s*$">
                    <attributes foreground="#FF<%= @base["0B"]["hex"] %>"/>
                </style>
                <style pattern="^\s*((?:[\w\.\-]|\\\s)+)\s*[=:]\s*">
                    <attributes foreground="#FF<%= @base["08"]["hex"] %>"/>
                </style>
                <style pattern="^\s*(?:[\w\.\-]|\\\s)+\s*[=:]\s*(\\u[\da-fA-F]{4})">
                    <attributes foreground="#FF<%= @base["0F"]["hex"] %>"/>
                </style>
                <style pattern="(\\)$">
                    <attributes foreground="#FF<%= @base["0D"]["hex"] %>"/>
                </style>
                <style pattern="([#!].*)$">
                    <attributes foreground="#FF<%= @base["04"]["hex"] %>"/>
                </style>
            </value>
        </entry>
    </styleMap>
    <defaultLineNumberBackground>#FF<%= @base["06"]["hex"] %></defaultLineNumberBackground>
    <defaultLineNumberForeground>#FF<%= @base["04"]["hex"] %></defaultLineNumberForeground>
    <defaultCodeBackground>#FF<%= @base["07"]["hex"] %></defaultCodeBackground>
    <defaultCodeForeground>#FF<%= @base["02"]["hex"] %></defaultCodeForeground>
    <defaultCaretColor>#FF<%= @base["02"]["hex"] %></defaultCaretColor>
    <defaultSelectionColor>#FF<%= @base["0D"]["hex"] %></defaultSelectionColor>
    <defaultSelectedTextColor>#FF<%= @base["06"]["hex"] %></defaultSelectedTextColor>
    <defaultErrorColor>#FF<%= @base["08"]["hex"] %></defaultErrorColor>
    <defaultWarningColor>#FF<%= @base["09"]["hex"] %></defaultWarningColor>
    <defaultInformationColor>#FF<%= @base["03"]["hex"] %></defaultInformationColor>
</ns2:syntaxTheme>
